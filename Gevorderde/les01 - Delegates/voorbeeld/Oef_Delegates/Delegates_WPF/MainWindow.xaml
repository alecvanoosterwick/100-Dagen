<Window x:Class="Delegates_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Delegates_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" FontSize="25">
    <DockPanel>
    <Grid DockPanel.Dock="Bottom">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button x:Name="btnPrint" Content="Print" Click="BtnPrint_Click"   Grid.Column="0" Margin="5"/>
        <Button x:Name="btnDelegateAlsParameter" Content="Delegate als Parameter" Click="BtnDelegateAlsParameter_Click"  
                Grid.Column="1" Margin="5"/>
    </Grid>
        <Label x:Name="lblResultaat" />
    </DockPanel>
</Window>
